{
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "**/__snapshots__/**"
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "types": ["node","jest"],
    "noEmit": true
  }
}
